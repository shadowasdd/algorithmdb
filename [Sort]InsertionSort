//Notes:
//insertion sort is a efficient algorithm for small number of elements
//O(n^2)

public class InsertionSort {
	//algorithm
	public double[] doInsertionSort(double[] sequence){
		int length=sequence.length;
		double key;
		for(int i=1;i<length;i++){
			key=sequence[i];
			int j=i-1;
			while(j>=0 && sequence[j]>key){
				sequence[j+1]=sequence[j];
				j=j-1;
			}
			sequence[j+1]=key;
		}
		return sequence;
	}
	
	//test main function
	public static void main(String[] args) {
		double[] sequence={-1,5,2,4,6,1,3};
		double[] permutation=new double[sequence.length];
		InsertionSort sort=new InsertionSort();
		permutation=sort.doInsertionSort(sequence);
		for(int i=0;i<sequence.length;i++){
			System.out.print(permutation[i]+",");
		}
	}

}
